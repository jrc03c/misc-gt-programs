-- outputs:
--     `r` = a random number in the range [0, 1)
--------------------------------------------------------------------------------

>> r = "0."

*repeat: 16
	*randomize
		*everytime
		>> r = "{r}1"
		>> r = "{r}2"
		>> r = "{r}3"
		>> r = "{r}4"
		>> r = "{r}5"
		>> r = "{r}6"
		>> r = "{r}7"
		>> r = "{r}8"
		>> r = "{r}9"
		>> r = "{r}0"
		
>> r = r + 0
*return